function Instancee(Class,Properties)
	local Part = Instance.new(Class)
	for i,v in pairs(Properties) do
		Part[i] = v
	end
	return Part
end

function Clonee(Part,Properties)
	local ClonePart = Part:Clone()
	for i,v in pairs(Properties) do
		ClonePart[i] = v
	end
	return(ClonePart)
end

function Instanceneww(Class,Properties)
	local Part = Instance.new(Class)
	for i,v in pairs(Properties) do
		Part[i] = v
	end
	return Part
end

